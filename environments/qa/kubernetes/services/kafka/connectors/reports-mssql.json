{
    "name": "reports-mssql",
    "config": {
        "connector.class": "io.debezium.connector.sqlserver.SqlServerConnector",
        "transforms.Reroute2.type": "io.debezium.transforms.ByLogicalTableRouter",
        "topic.creation.default.partitions": "12",
        "transforms.Reroute2.key.enforce.uniqueness": "false",
        "tasks.max": "1",
        "database.history.kafka.topic": "dbhistory.reservation",
        "transforms.Reroute.key.enforce.uniqueness": "false",
        "transforms": "Reroute, Reroute2, Reroute3",
        "transforms.Reroute2.topic.replacement": "center01p.dbo.rooms",
        "transforms.Reroute.topic.replacement": "center01p.dbo.reservations",
        "transforms.Reroute3.key.enforce.uniqueness": "false",
        "transforms.Reroute3.type": "io.debezium.transforms.ByLogicalTableRouter",
        "transforms.Reroute3.topic.replacement": "center01p.dbo.accounts",
        "transforms.Reroute3.topic.regex": "(.*)(account).*",
        "decimal.handling.mode": "double",
        "topic.creation.default.replication.factor": "3",
        "value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "key.converter": "org.apache.kafka.connect.json.JsonConverter",
        "database.hostname": "${file:/secrets/center-db/secrets.yaml:hostname}",
        "database.password": "${file:/secrets/center-db/secrets.yaml:password}",
        "database.server.name": "${file:/secrets/center-db/secrets.yaml:dbname}",
        "database.dbname": "${file:/secrets/center-db/secrets.yaml:dbname}",
        "database.user": "${file:/secrets/center-db/secrets.yaml:username}",
        "database.port": "${file:/secrets/center-db/secrets.yaml:port}",
        "transforms.Reroute.type": "io.debezium.transforms.ByLogicalTableRouter",
        "database.history.kafka.bootstrap.servers": "kafka:9092",
        "transforms.Reroute2.topic.regex": "(.*)(dbo.room|roomClass).*",
        "transforms.Reroute.topic.regex": "(.*)(reservation|notes|Vehicle|Precheckin|PrecheckOut|roomInventory|additional_guest).*",
        "value.converter.schemas.enable": "false",
        "name": "reports-mssql",
        "table.include.list": "dbo.reservation, dbo.roomInventoryExclude, dbo.reservation_address_ref, dbo.account_ref, dbo.roomblock_ref, dbo.payment_trans, dbo.notes_ref, dbo.Vehicle, dbo.additional_guest, dbo.room, dbo.user_ref, dbo.Precheckin, dbo.PrecheckOut, dbo.roomClass_published, dbo.roomClass_ref, dbo.account_room_xref, dbo.account_address_ref,dbo.ReservationCustom",
        "snapshot.mode": "schema_only",
        "message.key.columns":"dbo.room:roomClassID; dbo.roomClass_published:roomClassID; dbo.roomClass_ref:roomClassID; dbo.reservation:reservationId; dbo.ReservationCustom:reservationId; dbo.roomInventoryExclude:reservationId; dbo.reservation_address_ref:reservationId; dbo.Precheckin:reservationId; dbo.PrecheckOut:reservationId",
        "errors.log.include.messages": "true",
        "errors.log.enable": "true",
        "internal.key.converter": "org.apache.kafka.connect.json.JsonConverter",
        "internal.key.converter.schemas.enable": "false",
        "internal.value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "internal.value.converter.schemas.enable": "false",
        "key.converter.schemas.enable": "false"
      }
}