{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": [
                "arn:aws:s3:::steblynskyi-dev-pms-roomclass-images",
                "arn:aws:s3:::steblynskyi-dev-homeaway-rental-agreements"
            ]
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject",
                "s3:PutObjectAcl"
            ],
            "Resource": [
                "arn:aws:s3:::steblynskyi-dev-pms-roomclass-images/*",
                "arn:aws:s3:::steblynskyi-dev-homeaway-rental-agreements/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::steblynskyi-dev-store-inncenter*",
                "arn:aws:s3:::steblynskyi-dev-store-inncenter/*",
                "arn:aws:s3:::steblynskyi-dev-identity-documents",
                "arn:aws:s3:::steblynskyi-dev-identity-documents/*"
            ]
        }
    ]
}