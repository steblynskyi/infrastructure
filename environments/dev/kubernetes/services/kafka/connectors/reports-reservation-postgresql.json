{
    "name": "reports-reservation-postgresql",
    "config": {
      "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
      "errors.log.include.messages": "true",
      "topic.creation.default.partitions": "12",
      "slot.name": "pg_reservations_replication_slot",
      "tasks.max": "1",
      "transforms.Reroute.key.enforce.uniqueness": "false",
      "transforms": "Reroute",
      "internal.key.converter.schemas.enable": "false",
      "transforms.Reroute.topic.replacement": "reservation.public.customfields",
      "decimal.handling.mode": "double",
      "heartbeat.action.query": "INSERT INTO public.debezium_heartbeat (last_heartbeat_ts) VALUES (NOW());",
      "topic.creation.default.replication.factor": "3",
      "value.converter": "org.apache.kafka.connect.json.JsonConverter",
      "errors.log.enable": "true",
      "key.converter": "org.apache.kafka.connect.json.JsonConverter",
      "publication.autocreate.mode": "filtered",
      "transforms.Reroute.type": "io.debezium.transforms.ByLogicalTableRouter",
      "database.dbname": "${file:/secrets/reservation-db/secrets.yaml:dbname}",
      "database.user": "${file:/secrets/reservation-db/secrets.yaml:username}",
      "database.server.name": "${file:/secrets/reservation-db/secrets.yaml:dbname}",
      "database.port": "${file:/secrets/reservation-db/secrets.yaml:port}",
      "database.hostname": "${file:/secrets/reservation-db/secrets.yaml:hostname}",
      "database.password": "${file:/secrets/reservation-db/secrets.yaml:password}",
      "database.history.kafka.bootstrap.servers": "kafka:9092",
      "time.precision.mode": "connect",
      "heartbeat.interval.ms": "30000",
      "transforms.Reroute.topic.regex": "(.*)(customfield).*",
      "plugin.name": "pgoutput",
      "internal.key.converter": "org.apache.kafka.connect.json.JsonConverter",
      "key.converter.schemas.enable": "false",
      "internal.value.converter.schemas.enable": "false",
      "internal.value.converter": "org.apache.kafka.connect.json.JsonConverter",
      "value.converter.schemas.enable": "false",
      "name": "reports-reservation-postgresql",
      "table.include.list": "public.customfield, public.customfieldconfigurationmapping",
      "snapshot.mode": "never"
    }
}

