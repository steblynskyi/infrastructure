{
    "name": "reports-mssql",
    "config": {
        "database.history.kafka.bootstrap.servers": "kafka:9092",
        "connector.class": "io.debezium.connector.sqlserver.SqlServerConnector",
        "database.dbname": "here",
        "database.user": "${file:/secrets/inncenter-db/secrets.yaml:username}",
        "tasks.max": "1",
        "database.history.kafka.topic": "dbhistory.reservation",
        "database.server.name": "here",
        "database.port": "1433",
        "decimal.handling.mode": "double",
        "database.hostname": "sql-inncenter.steblynskyi.com",
        "database.password": "${file:/secrets/inncenter-db/secrets.yaml:password}",
        "value.converter.schemas.enable": "false",
        "table.include.list": "dbo.reservation, dbo.roomInventoryExclude, dbo.reservation_address_ref, dbo.additional_guest, dbo.Precheckin, dbo.PrecheckOut, dbo.user_ref, dbo.roomblock_ref, dbo.account_ref, dbo.room, dbo.roomClass_published, dbo.roomClass_ref, dbo.account_room_xref, dbo.account_address_ref, dbo.notes_ref, dbo.Vehicle, dbo.ReservationCustom, dbo.payment_trans",
        "value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "key.converter": "org.apache.kafka.connect.json.JsonConverter",
        "snapshot.mode": "schema_only",
        "transforms": "Reroute, Reroute2, Reroute3",
        "transforms.Reroute.type": "io.debezium.transforms.ByLogicalTableRouter",
        "transforms.Reroute.topic.regex": "(.*)(reservation|notes|Vehicle|Precheckin|PrecheckOut|roomInventory|additional_guest).*",
        "transforms.Reroute.topic.replacement": "here.dbo.reservations",
        "transforms.Reroute.key.enforce.uniqueness": "false",
        "transforms.Reroute2.type": "io.debezium.transforms.ByLogicalTableRouter",
        "transforms.Reroute2.topic.regex": "(.*)(dbo.room|roomClass).*",
        "transforms.Reroute2.topic.replacement": "here.dbo.rooms",
        "transforms.Reroute2.key.enforce.uniqueness": "false",
        "transforms.Reroute3.type": "io.debezium.transforms.ByLogicalTableRouter",
        "transforms.Reroute3.key.enforce.uniqueness": "false",
        "transforms.Reroute3.topic.replacement": "here.dbo.accounts",
        "transforms.Reroute3.topic.regex": "(.*)(account).*",
        "topic.creation.default.replication.factor": 3,
        "topic.creation.default.partitions": 5,
        "message.key.columns":"dbo.room:roomClassID; dbo.roomClass_published:roomClassID; dbo.roomClass_ref:roomClassID; dbo.notes_ref:referenceID; dbo.ReservationCustom:reservationId; dbo.Vehicle:ReservationConfirmationNumber;dbo.account_ref:accountID; dbo.account_room_xref:accountID; dbo.account_address_ref:accountID; dbo.reservation:reservationId; dbo.roomInventoryExclude:reservationId; dbo.reservation_address_ref:reservationId; dbo.Precheckin:reservationId; dbo.PrecheckOut:reservationId; dbo.additional_guest:referenceID;",
        "internal.key.converter": "org.apache.kafka.connect.json.JsonConverter",
        "internal.key.converter.schemas.enable": "false",
        "internal.value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "internal.value.converter.schemas.enable": "false",
        "key.converter.schemas.enable": "false"
    }
}